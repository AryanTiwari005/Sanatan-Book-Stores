    <script>
        window.onload = function() {
            const now = new Date();
            const hours = now.getHours();
            const greeting = document.querySelector(".home h2");

            if (hours < 12) {
                greeting.textContent = "Good Morning! Welcome to Sanatan Dharma Books";
            } else if (hours < 18) {
                greeting.textContent = "Good Afternoon! Welcome to Sanatan Dharma Books";
            } else {
                greeting.textContent = "Good Evening! Welcome to Sanatan Dharma Books";
            }
        };

        function redirectToAmazon(url) {
            window.location.href = url;
        }
    </script>
